<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="220" d:DesignHeight="120"
        x:Class="GabrielList.Window1"
		x:Name="wind"
        Title="wind"
		Width="220" Height="120">
	<Grid ShowGridLines="False" ColumnDefinitions="10 200 10" RowDefinitions="10 50 50 10">
		<StackPanel Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Left">
			<TextBlock x:Name="name" Margin="5 5" FontSize="20"></TextBlock>
		</StackPanel>
		<StackPanel Grid.Column="1" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Left">
			<TextBlock x:Name="date" Margin="5 5" FontSize="20"></TextBlock>
		</StackPanel>
	</Grid>
</Window>
